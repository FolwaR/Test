execute pathogen#infect()
colorscheme distinguished
set ts=4
set sw=4
set expandtab
set hlsearch
syntax on
set number
set listchars=tab:»\ ,extends:›,precedes:‹,nbsp:·,trail:·
set list
set smartcase
set incsearch
set showmatch
set wildmenu
filetype indent on
set cursorline
set showcmd

let NERDTreeShowHidden=1
au VimEnter * NERDTree
filetype plugin on
filetype plugin indent on







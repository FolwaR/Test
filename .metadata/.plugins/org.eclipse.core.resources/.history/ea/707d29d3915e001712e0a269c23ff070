int main() {

	struct st1
	{
		char *name[10];
		int lat;
	};

	st1 chuj1;

	chuj1.name[0] = "Kazik";
	chuj1.name[1] = "Ze zgorzelca";
	cout <<chuj1.name[0]<<chuj1.name[1];

	return 0;
}
